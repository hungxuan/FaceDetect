<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nguye.detec_2_4_9">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Face Detection"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.Home"
            android:label="Menu"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Create"
            android:label="Create"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Activity.Check"
            android:label="Check"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Activity.ListFace"
            android:label="List Faces" />
        <activity
            android:name=".Activity.ListStudent"
            android:label="Attendance"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>